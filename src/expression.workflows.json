[
  {
    "get_message": {
      "assign": [
        {
          "name": "Harsha"
        },
        {
          "last_name": "Kapoor"
        },
        {
          "full_name": "${name+\" \"+last_name}"
        },
        {
          "temperature_C": 27
        },
        {
          "temperature_F": "${temperature_C * 9/5 + 32}"
        },
        {
          "temperature_msg": "${\"Current temperature is \"+string(temperature_F)+\" F\"}"
        }
      ]
    }
  },
  {
    "return_value": {
      "return": "${temperature_msg}"
    }
  }
]