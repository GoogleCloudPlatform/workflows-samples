[
  {
    "create_table_entry": {
      "assign": [
        {
          "entry": [
            "123"
          ]
        }
      ]
    }
  },
  {
    "create_transaction": {
      "assign": [
        {
          "req": {
            "mutations": [
              {
                "insert": {
                  "columns": [
                    "ID"
                  ],
                  "table": "Contacts",
                  "values": [
                    "${entry}"
                  ]
                }
              }
            ],
            "singleUseTransaction": {
              "readWrite": {}
            }
          }
        }
      ]
    }
  },
  {
    "create_spanner_session": {
      "call": "googleapis.spanner.v1.projects.instances.databases.sessions.create",
      "args": {
        "database": "projects/PROJECT_ID/instances/SPANNER_INSTANCE/databases/SPANNER_DATABASE"
      },
      "result": "session"
    }
  },
  {
    "write_entry": {
      "call": "googleapis.spanner.v1.projects.instances.databases.sessions.commit",
      "args": {
        "session": "${session.name}",
        "body": "${req}"
      }
    }
  },
  {
    "delete_spanner_session": {
      "call": "googleapis.spanner.v1.projects.instances.databases.sessions.delete",
      "args": {
        "name": "${session.name}"
      }
    }
  },
  {
    "the_end": {
      "return": "SUCCESS"
    }
  }
]
