{
  "main": {
    "params": [
      "args"
    ],
    "steps": [
      {
        "new_annotation": {
          "call": "http.post",
          "args": {
            "url": "${\"https://vision.googleapis.com/v1/images:annotate\"}",
            "auth": {
              "type": "OAuth2"
            },
            "body": {
              "requests": "${args.requests}"
            }
          },
          "result": "vision_response"
        }
      },
      {
        "assign_vision": {
          "assign": [
            {
              "vision_result": "${vision_response.body.responses[0].labelAnnotations[0]}"
            }
          ]
        }
      },
      {
        "return_result": {
          "return": "${vision_result}"
        }
      }
    ]
  }
}